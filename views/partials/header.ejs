<script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc="
    crossorigin="anonymous"></script>
<script>
    $(document).ready(async () => {
        let recipes = [];
        await $.ajax({
            type: "get",
            url: "http://localhost:5000/get/all/recipe",
            success: (res) => {
                let recipes = res.res;
                console.log(recipes);
                $("#search").keyup(() => {
                    let test = $("#search").val();
                    recipes.forEach(recipe => {
                        let name = recipe.name.toLowerCase();
                        if (name.includes(test.toLowerCase())) {
                            console.log(recipe);
                            // name.style.display= recipes; 
                        }
                    })
                })
            },
            error: (error) => {
                console.log(error);
            }
        });

    })
</script>